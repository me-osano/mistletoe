#!/bin/bash

# Overwrite all the user configs in ~/.config/hypr with the Mistletoe defaults.

mistletoe-refresh-config hypr/autostart.conf
mistletoe-refresh-config hypr/bindings.conf
mistletoe-refresh-config hypr/input.conf
mistletoe-refresh-config hypr/looknfeel.conf
mistletoe-refresh-config hypr/hyprland.conf
